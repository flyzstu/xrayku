{
  "log": {
    "loglevel": "info"
  }, 
  "inbounds": [
    {
      "port": $PORT, 
      "protocol": "vless", 
      "settings": {
        "decryption": "none", 
        "clients": [
          {
            "id": $ID
          }
        ]
      }, 
      "streamSettings": {
        "network": "ws", 
        "security": "none"
      }
    }
  ], 
  "outbounds": [
    {
      "protocol": "freedom", 
      "tag": "Proxy"
    }, 
    {
      "protocol": "blackhole", 
      "settings": {
        "response": {
          "type": "http"
        }
      }, 
      "tag": "Reject"
    }
  ]
}